@import "../../../utils/colorui/main.wxss";
@import "../../../utils/colorui/icon.wxss";
page{
  width: 100%;
  height: 100%;
  background: #fff;
}
.num{
  margin: 200rpx 0;
  text-align: center;
  font-size: 300rpx;
  color: #009cff;
}
.list{
  width: 100%;
  float: left;
}
.li{
  height: 100rpx;
  border-top: 1px solid #eee;
  
}
.li:last-child{
  border-bottom: 1px solid #eee; 
}
.top{
  width: 50rpx;
  height: 50rpx;
  float: left;
  margin: 25rpx 0 25rpx 50rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 100%;
  background: #009cff;
  color: #fff;
}
.data{
  width: 60rpx;
  height: 60rpx;
  font-size: 42rpx;
  line-height: 60rpx;
  float: right;
  text-align: center;
  margin: 20rpx;
  z-index: -1;
}
.min{
  background: #C5EDD7;
}
.tip{
  float: left;
  font-size: 28rpx;
  line-height: 100rpx;
  margin-left: 24rpx;
}
.info{
  text-align: center;
}
.control{
  height: 100rpx;
  width: 80%;
  margin: 20rpx auto;
  text-align:center;
}
.btn{
  width: 25%;
  height: 80rpx;
  font-size: 40rpx;
  line-height: 80rpx;
  margin: 10rpx 20rpx;
  float: left;
}